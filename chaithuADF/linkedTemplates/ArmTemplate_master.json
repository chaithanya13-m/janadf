{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "chaithuADF"
		},
		"AzureBlobStorageDest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageDest'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabaseLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLS'"
		},
		"AzureSqlDatabaseSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseSource'"
		},
		"SqlServerAdventureWorks_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerAdventureWorks'"
		},
		"SqlServerAdventureWorks_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerAdventureWorks'"
		},
		"SqlServerLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLS'"
		},
		"SqlServerLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerLS'"
		},
		"SqlServerAdventureWorks_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-IGTQPFG\\Happy Users"
		},
		"SqlServerLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-IGTQPFG\\Happy Users"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "chaithuADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageDest_connectionString": {
						"value": "[parameters('AzureBlobStorageDest_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabaseLS_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLS_connectionString')]"
					},
					"AzureSqlDatabaseSource_connectionString": {
						"value": "[parameters('AzureSqlDatabaseSource_connectionString')]"
					},
					"SqlServerAdventureWorks_connectionString": {
						"value": "[parameters('SqlServerAdventureWorks_connectionString')]"
					},
					"SqlServerAdventureWorks_password": {
						"value": "[parameters('SqlServerAdventureWorks_password')]"
					},
					"SqlServerLS_connectionString": {
						"value": "[parameters('SqlServerLS_connectionString')]"
					},
					"SqlServerLS_password": {
						"value": "[parameters('SqlServerLS_password')]"
					},
					"SqlServerAdventureWorks_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerAdventureWorks_properties_typeProperties_userName')]"
					},
					"SqlServerLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}